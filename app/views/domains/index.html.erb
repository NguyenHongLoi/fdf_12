<div class="row">
  <div class="col-md-12 col-sm-12">
    <div class="">
      <% if @domain.present? %>
        <%= link_to t("common.create_button"), new_domain_path(domain_id: @domain.id),
          class: "btn btn-block btn-success btn-sm btn-create-domain" %>
      <% else %>
        <%= link_to t("common.create_button"), new_domain_path,
          class: "btn btn-block btn-success btn-sm btn-create-user" %>
      <% end %>
    </div>
  </div>
</div>
<table class="table table-striped table-hover table-bordered">
  <thead>
    <tr>
      <th><%= t "common.number" %></th>
      <th><%= t ".name" %></th>
      <th><%= t "members" %></th>
      <th><%= t "shop" %></th>
      <th><%= t "products" %></th>
      <th><%= t ".actions" %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @own_domains %>
    <%= render @member_domains %>
  </tbody>
</table>
